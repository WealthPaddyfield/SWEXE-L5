<% @tweets.each do |e| %>
    <%= e.message %>
    いいね数<%= e.like_users.count %><br>
    <%= link_to '削除', tweet_path(e), data:{turbo_method: :delete} %>
    
    <% if session[:login_uid] %>
        <%= link_to 'いいね', likes_path(tweet_id: e.id), data: { turbo_method: :post } %>
        <%= link_to 'いいね削除', like_path(e), data: { turbo_method: :delete } %>
    <% end %>
<br>
<% end %>

<% if session[:login_uid] %>
    <%= link_to "ツイート", new_tweet_path %>
<% end %>
<br>
<% if session[:login_uid].nil? %>
    <%= link_to '新規ユーザー登録', new_user_path %>
    <%= link_to 'ログイン', top_main_path, method: :post %>
<% end %>
<br>
<% if session[:login_uid] %>
    <%= link_to "マイプロフィール", profile_path %>
    <%= link_to "ログアウト", top_logout_path %>
<% end %>
<br>

loginUID : <%= session[:login_uid] %>